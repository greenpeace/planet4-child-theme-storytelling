{
  "name": "crate",
  "version": "4.0.0",
  "private": true,
  "description": "Starter WordPress theme from Cornershop Creative",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/cornershopcreative/wordpress"
  },
  "main": "index.js",
  "scripts": {
    "start": "gulp --continue",
    "build": "gulp build",
    "convert:sass": "sass-convert -C --indent t -R _src/scss/ -F scss -T sass && find _src/scss/ -name '*.scss' -type f -delete",
    "convert:scss": "sass-convert -C --indent t -R _src/scss/ -F sass -T scss && find _src/scss/ -name '*.sass' -type f -delete"
  },
  "keywords": [
    "wordpress",
    "gulp",
    "sass"
  ],
  "author": "Cornershop Creative",
  "license": "GPL-3.0",
  "browserify-shim": {
    "jQuery": "global:jQuery"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "browser-sync": "^2.13.0",
    "browserify": "^13.3.0",
    "browserify-shim": "^3.8.12",
    "cssnano": "^3.7.3",
    "debowerify": "^1.5.0",
    "del": "^2.2.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^3.0.1",
    "gulp-image-inline": "^1.1.1",
    "gulp-imagemin": "^3.0.1",
    "gulp-inject": "^4.1.0",
    "gulp-jslint": "^1.0.1",
    "gulp-match": "^1.0.2",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-svg-css": "^1.2.0",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgstore": "^6.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "include-media": "^1.4.9",
    "lodash.assign": "^4.0.9",
    "node-bourbon": "^4.2.8",
    "pageres": "^4.5.0",
    "path": "^0.12.7",
    "postcss-short": "^3.0.3",
    "postcss-sorting": "^1.6.1",
    "scut": "^1.4.0",
